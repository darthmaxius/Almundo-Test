{
  "presets": [
    "react",
    "es2015",
    "stage-0"
  ],
  "env": {
    "production": {
      "plugins": [
        "transform-class-properties",
        "transform-remove-debugger",
        [
          "transform-react-remove-prop-types",
          {
            "mode": "wrap"
          }
        ]
      ]
    }
  }
}
